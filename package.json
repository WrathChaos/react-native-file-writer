{
  "name": "react-native-file-writer",
  "version": "0.1.1",
  "description": "Easy to use export/write file with any format with the given data for React Native",
  "main": "./build/dist/FileWriter.js",
  "repository": "git@github.com:WrathChaos/react-native-file-writer.git",
  "author": "FreakyCoder <kurayogun@gmail.com>",
  "license": "MIT",
  "homepage": "https://www.freakycoder.com",
  "bugs": "https://github.com/WrathChaos/react-native-file-writer/issues",
  "keywords": [
    "file-writer",
    "file-export",
    "export",
    "file-system",
    "FreakyCoder",
    "freakycoder",
    "kuray",
    "Kuray",
    "react",
    "react-native",
    "javascript",
    "ui-lib",
    "rn"
  ],
  "scripts": {
    "build": "cd lib && tsc && cp ../package.json ../build/dist/ && Echo Build completed!",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prettier-format": "prettier --config .prettierrc.js '**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "rn-fetch-blob": ">= 0.12.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/react": "^16.9.53",
    "@types/react-native": "^0.63.25",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "react-native-typescript-transformer": "^1.2.13",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.js --fix-dry-run"
    ]
  }
}
